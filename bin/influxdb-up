#!/bin/bash

DOCKERNAME="influxdb"
NAME="influxdb"
STANDAR_PORT=8086
PORT=$STANDAR_PORT
VERSION="latest"

function new_container {
  IMAGE_EXTRA_OPTIONS="-v influx-data-$VERSION:/var/lib/mysql"
  docker run $DEFAULT_OPTIONS $IMAGE_EXTRA_OPTIONS $IMAGE_NAME
}

source $DOCKERDBMANAGERPATH/libexec/docker_wrapper_template
