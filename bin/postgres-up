#!/bin/bash

DOCKERNAME="postgres"
NAME="postgres"
STANDAR_PORT=5432
PORT=$STANDAR_PORT
VERSION="latest"

function new_container {
  IMAGE_EXTRA_OPTIONS="-v postgres-data-$VERSION:/var/lib/postgresql/data"
  docker run $DEFAULT_OPTIONS $IMAGE_EXTRA_OPTIONS $IMAGE_NAME
}

source $DOCKERWRAPPERS_ROOT/libexec/docker_wrapper_template
